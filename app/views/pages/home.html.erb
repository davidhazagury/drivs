<div class="img-container">
  <%= render 'landing_page/sections/landing-header' %>
  <%= render 'landing_page/sections/banner' %>
  <div class="btns-container">
    <div class="log-in-btn btn-prim">
      <%= link_to "Log in", new_user_session_path %>
    </div>
    <div class="sign-up-btn btn-second">
      <%= link_to "Sign up", new_user_registration_path %>
    </div>
  </div>
</div>
<section class="sec-one">
  <p class="headers sec-one-header">Your new way to rent a car</p>
  <div class="sec-one-content">
    <lottie-player src="https://assets1.lottiefiles.com/packages/lf20_FizrwJ.json" background="transparent"  speed="1"  style="width: 300px; height: 300px;" loop autoplay></lottie-player>
  </div>
</section>

<h1>Latest posts:</h1>
<ul>
  <% @latest_vehicules.each do |vehicule| %>
  <li>
    Brand: <%= vehicule.brand %>
  </li>
  <li>
    capacity: <%= vehicule.capacity %>
  </li>
  <li>
    Features: <%= vehicule.features %>
  </li>
  <li>
    Price: <%= vehicule.price %> $
  </li>
  <li>
    <%= link_to "Vehicule show", vehicule_path(vehicule.id)  %>
  </li>
  <% end %>
</ul>
